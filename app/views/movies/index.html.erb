<h1>Movies#index</h1>

<table >
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Rent price</th>
    </tr>
  </thead>
  <tbody>
    <% @movies.each do |movie| %>
      <tr>
        <td><%= link_to movie.title, movie %></td>
        <td><%= movie.description %></td>
        <td><%= movie.rent_price %></td>
      </tr>
    <% end %>
  </tbody>
</table>
